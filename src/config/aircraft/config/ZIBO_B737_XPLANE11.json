      },
      "wheelLightPosChange1": {
        "events": ["laminar/B738/switch/wheel_light_toggle"]
      }
    },
    "knob": {
      "com1": [
        {
          "inputEvents": ["lowerKnobClockwise"],
          "events": ["sim/radios/stby_com1_coarse_up"]
        },
        {
          "inputEvents": ["lowerKnobCounterclockwise"],
          "events": ["sim/radios/stby_com1_coarse_down"]
        },
        {
          "inputEvents": ["upperKnobClockwise"],
          "events": ["sim/radios/stby_com1_fine_up_833"]
        },
        {
          "inputEvents": ["upperKnobCounterclockwise"],
          "events": ["sim/radios/stby_com1_fine_down_833"]
        },
        {
          "inputEvents": ["push"],
          "events": ["sim/radios/com1_standy_flip"]
        },
        {
          "inputEvents": ["pull"],
          "events": ["sim/radios/com1_standy_flip"]
        }
      ],
      "com2": [
        {
          "inputEvents": ["lowerKnobClockwise"],
          "events": ["sim/radios/stby_com2_coarse_up"]
        },
        {
          "inputEvents": ["lowerKnobCounterclockwise"],
          "events": ["sim/radios/stby_com2_coarse_down"]
        },
        {
          "inputEvents": ["upperKnobClockwise"],
          "events": ["sim/radios/stby_com2_fine_up_833"]
        },
        {
          "inputEvents": ["upperKnobCounterclockwise"],
