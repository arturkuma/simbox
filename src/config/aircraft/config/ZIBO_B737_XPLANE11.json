          "inputEvents": ["lowerKnobClockwise", "upperKnobClockwise"],
          "events": ["sim/autopilot/airspeed_up"]
        },
        {
          "inputEvents": ["lowerKnobCounterclockwise", "upperKnobCounterclockwise"],
          "events": ["sim/autopilot/airspeed_down"]
        },
        {
          "inputEvents": ["push", "pull"],
          "events": ["sim/autopilot/knots_mach_toggle"]
        }
      ],
      "altitude": [
