        },
        {
          "inputEvents": ["push"],
          "events": ["laminar/B738/autopilot/lnav_press"]
        },
        {
          "inputEvents": ["pull"],
          "events": ["laminar/B738/autopilot/hdg_sel_press"]
        }
      ],
      "speed": [
