        },
        {
          "inputEvents": ["pull"],
          "events": ["sim/radios/com2_standy_flip"]
        }
      ],
      "nav1": [
        {
          "inputEvents": ["lowerKnobClockwise"],
          "events": ["sim/radios/stby_nav1_coarse_up"]
        },
        {
          "inputEvents": ["lowerKnobCounterclockwise"],
          "events": ["sim/radios/stby_nav1_coarse_down"]
        },
        {
          "inputEvents": ["upperKnobClockwise"],
          "events": ["sim/radios/stby_nav1_fine_up"]
        },
        {
          "inputEvents": ["upperKnobCounterclockwise"],
          "events": ["sim/radios/stby_nav1_fine_down"]
        },
        {
          "inputEvents": ["push"],
          "events": ["sim/radios/nav1_standy_flip"]
        },
        {
          "inputEvents": ["pull"],
          "events": ["sim/radios/nav1_standy_flip"]
        }
      ],
      "course": [
        {
          "inputEvents": ["lowerKnobClockwise", "upperKnobClockwise"],
          "events": ["laminar/B738/autopilot/course_pilot_up"]
        },
        {
          "inputEvents": ["lowerKnobCounterclockwise", "upperKnobCounterclockwise"],
          "events": ["laminar/B738/autopilot/course_pilot_dn"]
        }
      ],
      "heading": [
        {
          "inputEvents": ["lowerKnobClockwise", "upperKnobClockwise"],
